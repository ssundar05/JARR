<?xml version="1.0" encoding="utf-8" ?>
<!-- OPML generated by JARR on {{ now.isoformat() }} -->
<opml version="1.1">
  <head>
    <title>Feeds of {{ user.login }}</title>
    <dateCreated>{{ now.isoformat() }}</dateCreated>
    <dateModified>{{ now.isoformat() }}</dateModified>
    <ownerName>{{ user.login }}</ownerName>
    <ownerEmail>{{ user.email }}</ownerEmail>
  </head>
  <body>{% for feed in feeds %}
    <outline title="{{ feed.title|escape }}" text="{{ feed.title|escape }}" description="{{ feed.description|escape }}" {% if feed.category_id != None %}category="/{{ categories[feed.category_id].name }}"{% endif %} xmlUrl="{{ feed.link|escape }}" htmlUrl="{{ feed.site_link|escape }}" />{% endfor %}</body>
</opml>
